<launch>
    <!-- args -->
  <arg name="cmd_vel_out" default="cmd_vel"/>
  <arg name="config_locks" default="$(find agv_control)/config/twist_mux_locks.yaml"/>
  <arg name="config_topics" default="$(find agv_control)/config/twist_mux_topics.yaml"/>

    <!-- /joystick/cmd_vel and /navigation/cmd_vel node mix -->
  <node pkg="twist_mux" type="twist_mux" name="twist_mux" output="screen">
    <remap from="cmd_vel_out" to="$(arg cmd_vel_out)"/>
    <rosparam file="$(arg config_locks)" command="load"/>
    <rosparam file="$(arg config_topics)" command="load"/>
  </node>
    <!-- ypspur_ros -->
  <include file="$(find ypspur_ros)/launch/ypspur_ros.launch" />

    <!-- ps4_controller -->
  <!--<include file="$(find agv_control)/launch/agv_ps4_controller.launch" />-->
    <!-- key_vel -->
  <include file="$(find agv_control)/launch/agv_key_vel.launch" />

   <!-- one lidar-->
  <include file="$(find agv_control)/launch/test_urg.launch" />
   <!-- two lidar-->
  <!--<include file="$(find agv_control)/launch/agv_urg.launch" />-->
</launch>